{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Arial, Helvetica, sans-serif",
					"name": "Default Font",
					"slug": "default-font"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var:preset|color|white",
			"text": "var:preset|color|vivid-purple"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var:preset|color|vivid-cyan-blue"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "40px",
				"left": "40px",
				"right": "40px",
				"top": "40px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default-font)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "header",
			"area": "footer"
		}
	],
	"version": 2
}